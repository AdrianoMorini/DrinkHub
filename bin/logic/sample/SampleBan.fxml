<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="80.0" maxWidth="1200.0" minHeight="80.0" minWidth="1200.0" prefHeight="80.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="logic.view.TopBanViewController">
   <children>
      <ImageView fx:id="banImageId" fitHeight="88.0" fitWidth="1213.0" layoutY="-4.0" pickOnBounds="true" preserveRatio="false">
         <image>
            <Image url="logic/image/ban.JPG" />
         </image></ImageView>
      <Button fx:id="btnHome" layoutX="50.0" layoutY="28.0" mnemonicParsing="false" onAction="#goHome" text="Homepage" />
      <Button fx:id="btnSearch" layoutX="150.0" layoutY="28.0" mnemonicParsing="false" onAction="#searchPost" text="Search" />
      <Button fx:id="btnSearchUser" layoutX="250.0" layoutY="28.0" mnemonicParsing="false" onAction="#searchUser" text="Search User" />
      <Button fx:id="btnNewPost" layoutX="350.0" layoutY="28.0" mnemonicParsing="false" onAction="#newPost" text="New post" />
      <Button fx:id="btnUserPage" layoutX="450.0" layoutY="28.0" mnemonicParsing="false" onAction="#showUser" text="My Profile" />
      <Button fx:id="exit" layoutX="650.0" layoutY="28.0" mnemonicParsing="false" onAction="#exitPage" text="EXIT" />
   </children>
</AnchorPane>
